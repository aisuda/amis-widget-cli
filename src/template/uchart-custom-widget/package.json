          "图表组件",
          "跨端组件"
        ],
        "sort": 100,
        "device": [
          "mobile",
          "app"
        ]
      }
    },
    {
      "type": "uni-column-chart",
      "framework": "uniapp",
      "entry": "/src/components/uni-column-chart"
    },
    {
      "type": "uni-mount-chart",
      "framework": "vue3",
      "description": "山峰图",
      "entry": "/preview/uniMountRenderer.umd",
      "files": [
        "/preview/uniMountRenderer.css"
      ],
      "editorPlugin": {
        "pluginEntry": "/preview/uniMountPlugin.umd",
        "tag": [
          "图表组件",
          "跨端组件"
        ],
        "sort": 100,
        "device": [
          "mobile",
          "app"
        ]
      }
    },
    {
      "type": "uni-mount-chart",
      "framework": "uniapp",
      "entry": "/src/components/uni-mount-chart"
    },
    {
      "type": "uni-pie-chart",
      "framework": "vue3",
      "description": "饼状图",
      "entry": "/preview/uniPieRenderer.umd",
      "files": [
        "/preview/uniPieRenderer.css"
      ],
      "editorPlugin": {
        "pluginEntry": "/preview/uniPiePlugin.umd",
        "tag": [
          "图表组件",
          "跨端组件"
        ],
        "sort": 100,
        "device": [
          "mobile",
          "app"
        ]
      }
    },
    {
      "type": "uni-pie-chart",
      "framework": "uniapp",
      "entry": "/src/components/uni-pie-chart"
    },
    {
      "type": "uni-ring-chart",
      "framework": "vue3",
      "description": "环形图",
      "entry": "/preview/uniRingRenderer.umd",
      "files": [
        "/preview/uniRingRenderer.css"
      ],
      "editorPlugin": {
        "pluginEntry": "/preview/uniRingPlugin.umd",
        "tag": [
          "图表组件",
          "跨端组件"
        ],
        "sort": 100,
        "device": [
          "mobile",
          "app"
        ]
      }
    },
    {
      "type": "uni-ring-chart",
      "framework": "uniapp",
      "entry": "/src/components/uni-ring-chart"
    }
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/**/*.{js,jsx,ts,tsx,vue,scss,json}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://git@github.com:wibetter/uchart-custom-widget.git"
  },
  "bugs": {
    "url": "https://github.com/wibetter/uchart-custom-widget/issues"
  },
  "dependencies": {
    "@dcloudio/uni-app": "3.0.0-alpha-3070220230217001",
    "@dcloudio/uni-app-plus": "3.0.0-alpha-3070220230217001",
