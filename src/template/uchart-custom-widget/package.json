{
  "name": "uchart-custom-widget",
  "version": "1.2.10",
  "description": "图表自定义组件",
  "keywords": [
    "自定义组件",
    "uniapp自定义组件",
    "图表自定义组件"
  ],
  "author": "wibetter",
  "scripts": {
    "preview": "uni",
    "dev": "amis dev",
    "devDebug": "amis dev",
    "preview:h5": "amis preview",
    "linkDebug": "amis linkDebug",
    "build2lib": "amis build2lib",
    "dev:app": "uni -p app",
    "dev:custom": "uni -p",
    "dev:h5": "uni",
    "dev:h5:ssr": "uni --ssr",
    "dev:mp-alipay": "uni -p mp-alipay",
    "dev:mp-baidu": "uni -p mp-baidu",
    "dev:mp-kuaishou": "uni -p mp-kuaishou",
    "dev:mp-lark": "uni -p mp-lark",
    "dev:mp-qq": "uni -p mp-qq",
    "dev:mp-toutiao": "uni -p mp-toutiao",
    "dev:mp-weixin": "uni -p mp-weixin",
    "dev:quickapp-webview": "uni -p quickapp-webview",
    "dev:quickapp-webview-huawei": "uni -p quickapp-webview-huawei",
    "dev:quickapp-webview-union": "uni -p quickapp-webview-union",
    "build:app": "uni build -p app",
    "build:custom": "uni build -p",
    "build:h5:ssr": "uni build --ssr",
    "build:mp-alipay": "uni build -p mp-alipay",
    "build:mp-baidu": "uni build -p mp-baidu",
    "build:mp-kuaishou": "uni build -p mp-kuaishou",
    "build:mp-lark": "uni build -p mp-lark",
    "build:mp-qq": "uni build -p mp-qq",
    "build:mp-toutiao": "uni build -p mp-toutiao",
    "build:mp-weixin": "uni build -p mp-weixin",
    "build:quickapp-webview": "uni build -p quickapp-webview",
    "build:quickapp-webview-huawei": "uni build -p quickapp-webview-huawei",
    "build:quickapp-webview-union": "uni build -p quickapp-webview-union",
    "format": "prettier --write \"{src,build}/**/**/*.{js,jsx,ts,tsx,vue,scss,json}\""
  },
  "files": [
    "preview/*",
    "src/components/*"
  ],
  "aipage-widgets": [
    {
      "type": "uni-line-chart",
      "framework": "vue3",
      "description": "折线图",
      "entry": "/preview/uniLineRenderer.umd",
      "files": [
        "/preview/uniLineRenderer.css"
      ],
      "editorPlugin": {
        "pluginEntry": "/preview/uniLinePlugin.umd",
        "tag": [
          "图表组件",
          "跨端组件"
        ],
        "sort": 100,
        "device": [
          "mobile",
          "app"
        ]
      }
    },
    {
      "type": "uni-line-chart",
      "framework": "uniapp",
      "entry": "/src/components/uni-line-chart"
    },
    {
      "type": "uni-area-chart",
      "framework": "vue3",
      "description": "折线区域图",
      "entry": "/preview/uniAreaRenderer.umd",
      "files": [
        "/preview/uniAreaRenderer.css"
      ],
      "editorPlugin": {
        "pluginEntry": "/preview/uniAreaPlugin.umd",
        "tag": [
          "图表组件",
          "跨端组件"
        ],
        "sort": 100,
        "device": [
          "mobile",
          "app"
        ]
      }
    },
    {
      "type": "uni-area-chart",
      "framework": "uniapp",
      "entry": "/src/components/uni-area-chart"
    },
    {
      "type": "uni-column-chart",
      "framework": "vue3",
      "description": "柱状图",
      "entry": "/preview/uniColumnRenderer.umd",
      "files": [
        "/preview/uniColumnRenderer.css"
      ],
      "editorPlugin": {
